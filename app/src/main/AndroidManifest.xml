<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.cxf.mytest">

    <application
        android:name=".MyApplication"
        android:allowBackup="true"
        android:icon="@mipmap/icon1"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".SelectCourseActivity"/>
        <activity android:name=".ExitActivity"/>
        <activity android:name=".CourseInfoActivity"/>
        <activity android:name=".HelpActivity"/>
        <activity android:name=".QueryStudentInfoActivity"/>
        <activity android:name=".QueryStudentResultActivity"/>
        <activity android:name=".QueryCourseActivity"/>
        <activity android:name=".ChooseCourseActivity"/>
        <activity android:name=".CourseDetailsActivity"/>
    </application>

</manifest>